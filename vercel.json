{
  "framework": "vite",
  "buildCommand": "cd ai-cv-frontend && npm install && npm run build",
  "outputDirectory": "ai-cv-frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/$1.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}